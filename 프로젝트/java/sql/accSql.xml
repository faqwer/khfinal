<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="finaltp.acc">
	<insert id="accWrite2" parameterType="map">
		INSERT INTO accompany VALUES(Total_bbs_bbs_idx.currval, #{member_idx}, #{nation})
	</insert>
	
	<select id="accList2" resultType="finaltp.acc.model.AccDTO">
		<![CDATA[
		SELECT * FROM
			(SELECT ROWNUM AS rnum, a.* FROM
			(SELECT * FROM accompany ORDER BY bbs_idx desc)a)b
			WHERE rnum >= #{startnum} AND rnum <= #{endnum}
		]]>
	</select>
</mapper>