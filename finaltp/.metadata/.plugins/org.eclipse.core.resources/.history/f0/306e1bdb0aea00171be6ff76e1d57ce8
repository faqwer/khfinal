package finaltp.ask.model;

public class AskDTO {

	private int bbs_idx;
	private int member_idx;
	private String ask_status;

	public AskDTO() {
		super();
	}

	public AskDTO(int bbs_idx, int member_idx, String ask_status) {
		super();
		this.bbs_idx = bbs_idx;
		this.member_idx = member_idx;
		this.ask_status = ask_status;
	}

	public int getBbs_idx() {
		return bbs_idx;
	}

	public void setBbs_idx(int bbs_idx) {
		this.bbs_idx = bbs_idx;
	}

	public int getMember_idx() {
		return member_idx;
	}

	public void setMember_idx(int member_idx) {
		this.member_idx = member_idx;
	}

	public String getAsk_status() {
		return ask_status;
	}

	public void setAsk_status(String ask_status) {
		this.ask_status = ask_status;
	}

}
